  },
  "homepage": "https://github.com/jrobi133/memory-games#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^8.2.7",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "react-bootstrap": "^1.4.3",
    "react-router-dom": "^5.2.0"
  }
